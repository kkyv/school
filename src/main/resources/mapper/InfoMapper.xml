		<if test="id != null">
			AND t.id = #{id}
		</if>
		<if test="idList != null">
			AND 
			<foreach close=")" collection="idList" item="id" open="(" separator="OR">
				t.id = #{id}
			</foreach>
		</if>
		<if test="cateId != null">
			AND t.cate_id = #{cateId}
		</if>
	</sql>

	<select id="select" parameterType="Integer" resultMap="Info">
		SELECT 
		<include refid="fieldSql" />
		FROM <include refid="tableName" /> t
		WHERE t.id = #{id}
	</select>


	<delete id="delete" parameterType="Integer">
		DELETE FROM <include refid="tableName" />
		WHERE id = #{id}
	</delete>

	<select id="count" parameterType="cn.allene.school.po.condition.InfoCondition" resultType="Integer">
		SELECT 
			count(1)
		FROM <include refid="tableName" /> t
		 WHERE 1=1
		<include refid="whereSql" />
	</select>

	<select id="selectList" parameterType="cn.allene.school.po.condition.InfoCondition" resultMap="Info">
		SELECT 
		<include refid="fieldSql" />
		FROM <include refid="tableName" /> t
		WHERE 1=1
		<include refid="whereSql" />
	</select>

	<insert id="insert" keyColumn="id" keyProperty="id" parameterType="cn.allene.school.po.Info" useGeneratedKeys="true">
		INSERT INTO <include refid="tableName" />(
			id
		<if test="title != null">
			,title
		</if>
		<if test="content != null">
			,content
		</if>
		<if test="addTime != null">
			,add_time
		</if>
		)
		VALUES( 
			#{id}
		<if test="name != null">
			,#{name}
		</if>
		<if test="addTime != null">
			,#{addTime}
		</if>
		)
	</insert>

	<update id="update" parameterType="cn.allene.school.po.Class">
		UPDATE <include refid="tableName" /> t
		 <set>
             <if test="name != null">
                 t.name = #{name}
             </if>
             <if test="addTime != null">
                 t.add_time = #{addTime}
             </if>
	    </set>
		 WHERE t.id= #{id}
	</update>
</mapper>